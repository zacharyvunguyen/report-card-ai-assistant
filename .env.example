# API Keys
OPENAI_API_KEY=your_openai_key_here
LLAMA_PARSE_API_KEY=your_llamaparse_key_here
TAVILY_API_KEY=your_tavily_key_here

# Vector Database
REDIS_URL=redis://localhost:6379

# Embeddings and LLM Models
EMBED_MODEL=text-embedding-3-large
LLM_MODEL=gpt-4o
TEMPERATURE=0.2

# Chunking Parameters
CHUNK_SIZE=2048
CHUNK_OVERLAP=128

# Storage Options
STORAGE_MODE=local  # Options: local, gcs
GCS_BUCKET_NAME=your_bucket_name  # Required for gcs mode

# Logging
LOG_LEVEL=INFO 